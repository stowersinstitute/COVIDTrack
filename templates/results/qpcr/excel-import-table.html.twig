<table class="table table-bordered table-hover">
    <thead>
    <tr>
        <th>Specimen ID</th>
        <th>Conclusion</th>
        <th>Well Plate Barcode</th>
        <th>Well Position</th>
        <th>Ct / Amp Score</th>
    </tr>
    </thead>

    <tbody>
    {% for result in results  %}
        <tr>
            <td>{{ result.specimenAccessionId }}</td>
            <td>{{ result.conclusion }}</td>
            <td>{{ result.wellPlateBarcode }}</td>
            <td>{{ result.wellPosition }}</td>
            <td>
                Ct1: {{ result.CT1 }} / {{ result.CT1AmpScore }}<br>
                Ct2: {{ result.CT2 }} / {{ result.CT2AmpScore }}<br>
                Ct3: {{ result.CT3 }} / {{ result.CT3AmpScore }}
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>
