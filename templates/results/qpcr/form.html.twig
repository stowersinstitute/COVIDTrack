{% extends 'base.html.twig' %}

{% block page_title %}
    {% if new %}
        New Viral Result &ndash; {{ specimenAccessionId }}
    {% else %}
        Edit Viral Result &ndash; {{ result.specimenAccessionId }} on Plate {{ result.wellPlateBarcode }}
    {% endif %}
{% endblock %}

{% block body %}
<div class="row">
    <div class="col-md-6">
        <div class="box box-primary">
            <div class="box-body">
                {{ form(form) }}
            </div>
        </div>
    </div>
</div>
{% endblock %}
